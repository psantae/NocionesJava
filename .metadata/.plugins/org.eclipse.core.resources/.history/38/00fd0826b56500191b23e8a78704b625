package entidades;

public abstract class Empleado {
	private String nombre;
	private String DNI;
	private double sueldoBase;
	
	public abstract double calcularSueldo(); //si el metodo es abstracto la clase tambien debe serlo
	
	
	public String getNombre() {
		return nombre;
	}
	
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	
	public String getDNI() {
		return DNI;
	}
	
	public void setDNI(String dNI) {
		DNI = dNI;
	}
	
	public double getNomina() {
		return sueldoBase;
	}
	
	public void setNomina(double nomina) {
		this.sueldoBase = nomina;
	}
	
	@Override
	public String toString() {
		return "Empleado [nombre=" + nombre + ", DNI=" + DNI + ", nomina=" + sueldoBase + "]";
	}
	
	
}
